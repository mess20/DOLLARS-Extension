{
    "name": "DOLLARS Extension",
    "description": "A simple Chrome extension made for DOLLARS chatroom.",
    "version": "1.0",
    "manifest_version": 2,
  "icons": {
"16":"icon.svg",
"32":"icon.svg",
"48":"icon.svg",
"128":"icon.svg"
},
"browser_action": {
   "default_icon":"icon.svg"
  },
    "content_scripts": [
        { 
"matches":["https://drrr.com/room/*"], 
"js": ["jquery.js","content.js"]
}
    ]
}


